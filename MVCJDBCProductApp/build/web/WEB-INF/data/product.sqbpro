s (jika belum ada)
CREATE TABLE IF NOT EXISTS categories (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    name TEXT NOT NULL UNIQUE
);

-- 2. Tambah kolom category_id ke tabel products
ALTER TABLE products ADD COLUMN category_id INTEGER;

-- 3. Isi category_id berdasarkan kolom category lama (TEXT)
-- (ini otomatis konversi kategori lama ke ID)
UPDATE products SET category_id = (
    SELECT id FROM categories 
    WHERE categories.name = products.category 
    LIMIT 1
);

-- 4. Masukkan kategori yang belum ada di tabel categories
INSERT OR IGNORE INTO categories (name)
SELECT DISTINCT category FROM products WHERE category IS NOT NULL;

-- 5. Update ulang category_id kalau ada yang NULL
UPDATE products SET category_id = (
    SELECT id FROM categories 
    WHERE categories.name = products.category 
    LIMIT 1
) WHERE category_id IS NULL;

-- 6. (Opsional) Hapus kolom category lama agar rapi
-- ALTER TABLE products RENAME TO products_old;
